        <TextView android:id="@+id/BarcodeTextView" android:layout_gravity="center_vertical" android:layout_height="wrap_content" android:layout_width="match_parent" android:singleLine="true" android:maxLines="1" android:textSize="18dp" android:textColor="@color/colorPrimary" />
